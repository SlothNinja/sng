<!DOCTYPE html PUBLIC "-//W3C//DTD XHTML 1.0 Strict//EN" "http://www.w4.org/TR/xhtml1/DTD/xhtml1-strict.dtd"> 
<html xmlns="http://www.w3.org/1999/xhtml"> 
        <head>
                {{template "shared/head_meta" .}}
                {{template "shared/head_title" .}}
                {{template "shared/head_link" .}}
                <link href="/stylesheets/got-screen.css" media="screen, projection" rel="stylesheet" type="text/css" /> 
                {{template "shared/head_script" .}}
                <script src="/javascripts/got.js" type="text/javascript"></script> 
        </head> 
        <body class="got bp">
                <div id="container">
                        {{template "shared/header" .}}
                        <div id="upper-content">
                                {{template "got/status_box" .}}
                        </div> 
                        <div id="main-content">
                                {{template "got/board" .}}
                                <div class="content-box">
                                        <div class="content">{{template "got/player_table" .}}</div>
                                </div>
                        </div>
                        <div id="lower-content">
                                <div class="left-column">
                                        {{template "shared/gamelog2" .}}
                                </div> 
                                <div class="right-column">
                                        {{template "shared/messagelog" .}}
                                </div> 
                        </div>
                        {{template "shared/footer" .}}
                </div>
        </body>
</html>
